{
  "name": "arasaacapi",
  "version": "0.0.1",
  "private": true,
  "description": "New Swagger API Project",
  "keywords": [],
  "author": "",
  "license": "",
  "main": "app.js",
  "scripts": {
    "prestart": "npm run build",
    "serve": "node dist/app.js",
    "build": "rimraf ./dist; cp -r src dist; babel dist --out-dir dist --presets @babel/preset-env",
    "start": "cross-env DEBUG=swagger-tools:middleware:* nodemon --inspect=0.0.0.0:9229 --ignore 'src/public/arasaac.json' --ignore 'materials/' --ignore 'pictograms/' --ignore 'svg/' src/app.js --exec babel-node  --presets @babel/preset-env",
    "edit": "swagger_swagger_fileName=src/swagger/swagger.yaml swagger project edit",
    "test": "swagger project test",
    "compile": "yaml2json src/api/swagger/swagger.yaml > src/swagger.json"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.15.0",
    "connect-flash": "^0.1.1",
    "cors": "^2.8.4",
    "email-verification": "^0.4.6",
    "express": "^4.12.3",
    "express-session": "^1.14.1",
    "fs-extra": "^6.0.1",
    "imagemin": "5.3.1",
    "imagemin-pngquant": "5.1.0",
    "intl-locales-supported": "^1.0.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^4.4.6",
    "oauth2orize": "^1.11.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-http-bearer": "^1.0.1",
    "passport-twitter": "^1.0.4",
    "recursive-readdir": "^2.2.1",
    "request": "^2.83.0",
    "sharp": "^0.20.2",
    "stopword": "0.1.10",
    "stopwords-iso": "0.4.0",
    "swagger-express-mw": "^0.7.0",
    "yamljs": "^0.2.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "cross-env": "^3.0.0",
    "eslint": "^4.15.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.1",
    "should": "^7.1.0",
    "supertest": "^1.0.0"
  }
}
